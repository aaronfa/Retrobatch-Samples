using terms from application "Retrobatch"		(*	The processStart handler is called when your workflow has started running	*)	on processStart()		do shell script "echo name, width, height > ~/Desktop/rb_output.csv"	end processStart		(* 	The processAsset handler is called for each image asset that passes through this AppleScript node	You can return a boolean value that says wether or not this asset should continue on through the next node(s)	*)	on processAsset(anAsset)				set str to (name of anAsset) & "," & (width of anAsset as string) & "," & (height of anAsset as string)				do shell script "echo " & quoted form of (str as string) & " >> ~/Desktop/rb_output.csv"				return true			end processAsset	end using terms from